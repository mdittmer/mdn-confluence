{
  "name": "mdn-confluence",
  "version": "2.1.2",
  "description": "User support tool for maintaining MDN compatibility data using Web API Confluence data.",
  "scripts": {
    "generate": "node ./main/generate.es6.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "author": "Mark Dittmer",
  "contributors": [
    "Philip JÃ¤genstedt"
  ],
  "dependencies": {
    "compare-versions": "^3.6.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "curly": [
        "error",
        "all"
      ],
      "max-len": [
        "error",
        {
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreUrls": true
        }
      ],
      "require-jsdoc": "off"
    }
  }
}
